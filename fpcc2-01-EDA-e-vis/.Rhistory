require(ggplot2)
require(GGally)
require(dplyr)
posgraduandos <- read.csv("dados//Dados de alunos para as aulas de FPCC-report.csv")
posgraduandos <- posgraduandos %>% select(2, 3, 5, 6, 7, 9) %>% filter(complete.cases(posgraduandos))
names(posgraduandos) <- c("curso", "sexo", "linguagens", "repositorios", "estatistica", "altura")
str
str(posgraduandos)
ggplot(posgraduandos, aes(x = altura)) + geom_histogram()
ggplot(posgraduandos, aes(x = altura)) + geom_histogram(binwidth = 50)
ggplot(posgraduandos, aes(x = altura)) + geom_histogram(binwidth = 5)
ggplot(posgraduandos, aes(x = altura)) + geom_histogram(binwidth = 5) + geom_rug()
ggplot(posgraduandos, aes(x = altura)) + geom_histogram(binwidth = 10) + geom_rug()
ggplot(posgraduandos, aes(x = altura)) + geom_density() + geom_rug()
ggplot(posgraduandos, aes(x = altura)) + geom_density(adjust = 1/5) + geom_rug()
ggpairs(posgraduandos)
cor(posgraduandos$altura, posgraduandos$repositorios)
cor(posgraduandos$altura, posgraduandos$repositorios, method = "kendall")
cor(posgraduandos$altura, posgraduandos$repositorios, method = "spearman")
